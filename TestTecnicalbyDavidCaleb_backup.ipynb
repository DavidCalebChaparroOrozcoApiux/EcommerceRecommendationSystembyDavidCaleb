    "                # CORREGIDO: Pasar top_n como entero expl칤citamente\n",
    "                recommendations = generate_recommendations(\n",
    "                    recommender, user_id, user_to_idx, idx_to_item, user_item_df, \n",
    "                    top_n=int(top_n),  # Convertir expl칤citamente a entero\n",
    "                    method=method\n",
    "                )\n",
    "                \n",
